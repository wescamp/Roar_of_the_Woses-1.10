msgid "Ar'Kasan"
msgstr "Ar'Kasan"

#. [side]: type=Lich, id=ArKasan
#. [side]: type=Death Knight, id={ID}
#: Roar_of_the_Woses/scenarios/09_Crossroads.cfg:74
#: Roar_of_the_Woses/scenarios/09_Crossroads.cfg:90
